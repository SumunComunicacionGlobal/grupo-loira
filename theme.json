{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 2,
	"settings": {
		"appearanceTools": true,
		"useRootPaddingAwareAlignments": true,

		"color": {
			"custom": false,
			"defaultPalette": false,
			"defaultGradients": false,
			"customDuotone": false,
			"customGradient": false,
			"palette": [
				{
					"slug": "background",
					"color": "#FFFFFF",
					"name": "Background"
				},
				{
					"slug": "primary-10",
					"color": "#F2EBFF",
					"name": "Primary 10"
				},
				{
					"slug": "primary-30",
					"color": "#C8A8FF",
					"name": "Primary 30"
				},
				{
					"slug": "primary",
					"color": "#873FFA",
					"name": "Primary"
				},
				{
					"slug": "primary-80",
					"color": "#131B83",
					"name": "Primary 80"
				},
				{
					"slug": "primary-100",
					"color": "#070B41",
					"name": "Primary 100"
				},
                {
					"slug": "secondary-10",
					"color": "#FDE8EA",
					"name": "Secondary 10"
				},
				{
					"slug": "secondary-30",
					"color": "#FF9EBA",
					"name": "Secondary 30"
				},
				{
					"slug": "secondary",
					"color": "#FF8B00",
					"name": "Secondary"
				},
				{
					"slug": "secondary-80",
					"color": "#E20916",
					"name": "Secondary 80"
				},
				{
					"slug": "secondary-100",
					"color": "#A4101A",
					"name": "Secondary 100"
				},
                {
					"slug": "accent-10",
					"color": "#F9FFF6",
					"name": "Accent 10"
				},
				{
					"slug": "accent-30",
					"color": "#E6FFD8",
					"name": "Accent 30"
				},
				{
					"slug": "accent",
					"color": "#C6FFA4",
					"name": "Accent"
				},
				{
					"slug": "accent-80",
					"color": "#A8D98B",
					"name": "Accent 80"
				},
				{
					"slug": "accent-100",
					"color": "#8DB574",
					"name": "Accent 100"
				},
				{
					"slug": "gray-30",
					"color": "var(--wp--custom--color--neutral-30)",
					"name": "Gray"
				}
			],
			"gradients": [],
			"link": false
		},
		"custom": {
			"border-radius": {
				"zero": "0",
				"sm": "4px",
				"md": "8px",
                "lg": "16px",
                "xl": "32px",
				"rounded": "999px"
			},
			"border-width": {
				"tiny": "1px",
				"small": "2px",
				"medium": "3px",
				"large": "4px"
			},
			"box-shadow": {
				"1": "0px 2px 8px rgba(33, 33, 33, 0.12)",
				"2": "3px 3px 10px 10px rgba(0, 0, 0, 0.10)"
			},
			"color": {
				"link" : "var(--wp--preset--color--primary)",
				"neutral-white": "#FFFFFF",
				"neutral-10": "#EBEAEA",
				"neutral-20": "#D6D5D5",
				"neutral-30": "#C2C0C1",
				"neutral-40": "#AEABAC",
				"neutral-50": "#999697",
				"neutral-60": "#858082",
				"neutral-70": "#706B6D",
				"neutral-80": "#5C5658",
				"neutral-90": "#484144",
				"neutral-100": "#332C2F",
				"neutral-black": "#1F171A"
			},
			"layout": {
				"content": "1280px",
				"wide": "90%",
				"sidebar": "336px",
				"page": "var(--wp--custom--layout--content)",
				"padding": "16px",
				"block-gap": "2rem",
				"block-gap-large": "40px"
			},
			"letter-spacing": {
				"none": "normal",
				"tight": "-.01em",
				"loose": ".05em",
				"looser": ".1em"
			},
			"line-height": {
				"tiny": 1,
				"headings": 1.2,
				"normal": 1.5
			}
		},
		"spacing": {
			"blockGap": true,
			"margin": true,
			"padding": true,
			
			"units": [
				"%",
				"px",
				"em",
				"rem",
				"vh",
				"vw"
			],

			"spacingSizes": [
				{
					"size": "1rem",
					"slug": "10",
					"name": "Space 1 (1rem - 16px)"
				},

				{
					"size": "1.5rem",
					"slug": "20",
					"name": "Space 2 (1.5rem - 24px)"
				},
				{
					"size": "2rem",
					"slug": "30",
					"name": "Space 3 (2rem - 32px)"
				},
				{
					"size": "2.5rem",
					"slug": "40",
					"name": "Space 4 (2.5rem - 40px)"
				},
				{
					"size": "4rem",
					"slug": "60",
					"name": "Space 6 (4rem - 64px)"
				},
				{
					"size": "4.5rem",
					"slug": "70",
					"name": "Space 7 (4.5rem - 72px)"
				},
				{
					"size": "5rem",
					"slug": "80",
					"name": "Space 8 (5rem - 80px)"
				}
			]
		},
		"typography": {
			"dropCap": false,
			"lineHeight": false,
			"customFontSize": false,
			"fontWeight": false,
			"letterSpacing": false,
			"fontFamilies": [
				{
					"fontFace": [
						{
							"fontDisplay": "block",
							"fontFamily": "BW Nista",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "400",
							"src": [
								"file:./assets/fonts/BwNistaInt-Md.woff2"
							]
						},
						{
							"fontDisplay": "block",
							"fontFamily": "BW Nista",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "200",
							"src": [
								"file:./assets/fonts/BwNistaInt-Lt.woff2"
							]
						},
						{
							"fontDisplay": "block",
							"fontFamily": "BW Nista",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "bold",
							"src": [
								"file:./assets/fonts/BwNistaInt-xBd.woff2"
							]
						}
					],
					"fontFamily": "\"BW Nista\", sans-serif",
					"name": "Main Font",
					"slug": "main-font"
				}
			],
            
			"fontSizes": [
				{
					"name": "Display",
					"size": "clamp(3rem, 8.8vw, 8rem)",
					"slug": "display"
				},
				{
					"name": "H1",
					"size": "clamp(3rem, 4.8vw, 4rem)",
					"slug": "heading-1"
				},
				{
					"name": "H2",
					"size": "clamp(2rem, 4.4vw, 3rem)",
					"slug": "heading-2"
				},
				{
					"name": "H3",
					"size": "2rem",
					"slug": "heading-3"
				},
				{
					"name": "H4",
					"size": "1.5rem",
					"slug": "heading-4"
				},
				{
					"name": "H5",
					"size": "1.25rem",
					"slug": "heading-5"
				},
				{
					"name": "H6",
					"size": "1.125",
					"slug": "heading-6"
				},
				{
					"name": "Subtitle",
					"size": "clamp(0.875rem, 4.4vw, 1.5rem)",
					"slug": "subtitle"
				},
				{
					"name": "Body",
					"size": "clamp(1rem, 2.4vw, 1.125rem)",
					"slug": "body"
				},
				{
					"name": "Small",
					"size": "0.875rem",
					"slug": "small"
				},
				{
					"name": "Caption",
					"size": "0.75rem",
					"slug": "caption"
				}
			]
		},
		"layout": {
			"contentSize": "var(--wp--custom--layout--content)",
			"wideSize": "var(--wp--custom--layout--wide)"
		},
		"border": {
			"color": false,
			"radius": true,
			"style": false,
			"width": false
		},
		"blocks": {
			"core/button": {
				"border": {
					"radius": true
				}
			},
			"core/group": {
				"color": {
					"background": true
				}
			}
		}
	},
	"styles": {
		"color": {
			"background": "var(--wp--preset--color--background)",
			"text": "var(--wp--preset--color--foreground)"
		},
		"elements": {
			"link": {
				"color": {
					"text": "var(--wp--custom--color--link)"
				}
			},
			"button": {
				"color": {
					"background": "var(--wp--preset--color--primary)",
					"text": "var(--wp--preset--color--background)"
				},
				"border": {
					"width": "0px",
					"radius": "var(--wp--custom--border-radius--rounded)"
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--body)",
                    "fontFamily": "var(--wp--preset--font-family--main-font)",
					"lineHeight": "var(--wp--custom--line-height--normal)"
				},
				"spacing": {
					"padding": "calc(0.5rem + 4px) 1.5em"
				}
			},
			"heading": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--main-font)",
					"lineHeight": "var(--wp--custom--line-height--headings)"
				}
			},
			"h1": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--heading-1)",
					"lineHeight": "var(--wp--custom--line-height--tiny)"
				}
			},
			"h2": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--heading-2)"
				}
			},
			"h3": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--heading-3)"
				}
			},
			"h4": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--heading-4)"
				}
			},
			"h5": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--heading-5)"
				}
			},
			"h6": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--heading-6)"
				}
			}
		},
		"spacing": {
			"blockGap": "var(--wp--custom--layout--block-gap)",
			"padding": {
                "top": "0px",
                "right": "2rem",
                "bottom": "0px",
                "left": "2rem"
            }
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--main-font)",
			"lineHeight": "var(--wp--custom--line-height--normal)",
			"fontSize": "var(--wp--preset--font-size--body)"
		},

		"blocks": {
			"core/group": {
				"spacing": {
				}
			}
		}
	}
}
